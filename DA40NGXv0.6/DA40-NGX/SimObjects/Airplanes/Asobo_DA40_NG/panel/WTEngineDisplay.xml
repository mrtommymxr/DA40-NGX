	<EngineDisplaySystem>
	
		<Gauge><!--LOAD_Master-->
			<Type>Circular</Type>
			<ID>Piston_LoadGauge</ID>
			<Title></Title>
			<Unit>Load%</Unit>
			<Minimum>0</Minimum>
			<Maximum>100</Maximum>
			<Style>
				<ForceTextColor>white</ForceTextColor>
				<SizePercent>105</SizePercent>
				<Graduations>20,40,60,80</Graduations>
			</Style>
					<Value>
					<If>
					<Condition>
					<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
					</Condition>
					<Then>
						<Max>
							<Min>
								<Multiply>
									<Divide>
										<Multiply>
											<Simvar name="ENG TORQUE:1" unit="Foot pounds"/>
											<Divide>
												<Simvar name="GENERAL ENG RPM:1" unit="rpm"/>
												<Constant>5252</Constant>
											</Divide>
											<Constant>550</Constant>
										</Multiply>
											<Constant>92400</Constant>
									</Divide>
									<Constant>100</Constant>
								</Multiply>
								<Constant>100</Constant>
							</Min>
							<Constant>0</Constant>
						</Max>
					</Then>
					<Else>
						<Constant>-100</Constant>
					</Else>
					</If>
					</Value>
			<ColorZone>
				<Color>green</Color>
				<Begin>0</Begin>
				<End>92.5</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>92.5</Begin>
				<End>100</End>
			</ColorZone>
			<YellowBlink>
				<And>
				<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				<Greater>
					<Multiply>
							<Divide>
								<Multiply>
									<Simvar name="ENG TORQUE:1" unit="Foot pounds"/>
									<Divide>
										<Simvar name="GENERAL ENG RPM:1" unit="rpm"/>
										<Constant>5252</Constant>
									</Divide>
									<Constant>550</Constant>
								</Multiply>
									<Constant>92400</Constant>
							</Divide>
							<Constant>100</Constant>
						</Multiply>
					<Constant>92.5</Constant>
				</Greater>
				</And>
			</YellowBlink>
			<RedBlink>
				<And>
				<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				<Greater>
					<Multiply>
							<Divide>
								<Multiply>
									<Simvar name="ENG TORQUE:1" unit="Foot pounds"/>
									<Divide>
										<Simvar name="GENERAL ENG RPM:1" unit="rpm"/>
										<Constant>5252</Constant>
									</Divide>
									<Constant>550</Constant>
								</Multiply>
									<Constant>92400</Constant>
							</Divide>
							<Constant>100</Constant>
						</Multiply>
					<Constant>101</Constant>
				</Greater>
				</And>
			</RedBlink>
		</Gauge>
		  
		<Gauge><!--RPM_Master-->
			<Type>Circular</Type>
			<ID>Piston_RPMGauge</ID>
			<Title></Title>
			<Unit>RPM</Unit>
			<Minimum>0</Minimum>
			<Maximum>3000</Maximum>
			<Style>
				<TextIncrement>10</TextIncrement>
				<ForceTextColor>white</ForceTextColor>
				<SizePercent>105</SizePercent>
				<Graduations>600,1200,1800,2400</Graduations>
				
			</Style>
			<Value>
			<If>
				<Condition>
					<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				</Condition>
			<Then>
				<Simvar name="PROP RPM:1" unit="rpm"/>
			</Then>
			<Else>
				<Constant>-3000</Constant>
			</Else>
			</If>
			</Value>
			<ColorZone>
				<Color>white</Color>
				<Begin>0</Begin>
				<End>3000</End>
			</ColorZone>
			<ColorZone>
				<Color>green</Color>
				<Begin>0</Begin>
				<End>2105</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>2105</Begin>
				<End>2300</End>
			</ColorZone>
			<ColorZone>
				<Color>red</Color>
				<Begin>2305</Begin>
				<End>3000</End>
			</ColorZone>
			<YellowBlink>
				<And>
				<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				<Greater>
					<Simvar name="PROP RPM:1" unit="rpm"/>
					<Constant>2105</Constant>
				</Greater>
				</And>
			</YellowBlink>
			<RedBlink>
				<And>
				<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				<Greater>
					<Simvar name="PROP RPM:1" unit="rpm"/>
					<Constant>2305</Constant>
				</Greater>
				</And>
			</RedBlink>
		</Gauge>
		
		<Header><!--Electircal-->
			<Text>Electrical</Text>
		</Header>
		
		<Gauge><!--Volts-->
			<Type>Horizontal</Type>
			<ID>Voltage_Gauge</ID>
			<Title>Volts</Title>
			<Unit></Unit>
			<Minimum>23</Minimum>
			<Maximum>34</Maximum>
			<Style>
					<ForceTextColor>white</ForceTextColor>
					<SizePercent>100</SizePercent>
					<ValuePos>End</ValuePos>
			</Style>
			<Value>
				<Simvar name="ELECTRICAL MAIN BUS VOLTAGE:3" unit="volts"/>
			</Value>
			<ColorZone>
				<Color>red</Color>
				<Begin>23</Begin>
				<End>24.1</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>24.1</Begin>
				<End>25</End>
			</ColorZone>
			<ColorZone>
				<Color>green</Color>
				<Begin>25</Begin>
				<End>30</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>30</Begin>
				<End>32</End>
			</ColorZone>
			<ColorZone>
				<Color>red</Color>
				<Begin>32</Begin>
				<End>34</End>
			</ColorZone>
			<BeginText></BeginText>
			<EndText></EndText>
			<YellowBlink>
				<Or>
					<Greater>
						<Simvar name="ELECTRICAL MAIN BUS VOLTAGE:3" unit="volts"/>
						<Constant>30</Constant>
					</Greater>
					<Lower>
						<Simvar name="ELECTRICAL MAIN BUS VOLTAGE:3" unit="volts"/>
						<Constant>25</Constant>
					</Lower>
				</Or>
			</YellowBlink>
			<RedBlink>
				<Or>
					<Greater>
						<Simvar name="ELECTRICAL MAIN BUS VOLTAGE:3" unit="volts"/>
						<Constant>32</Constant>
					</Greater>
					<Lower>
						<Simvar name="ELECTRICAL MAIN BUS VOLTAGE:3" unit="volts"/>
						<Constant>24.1</Constant>
					</Lower>
				</Or>
			</RedBlink>
		</Gauge>
		
		<Gauge><!--AMPS-->
			<Type>Horizontal</Type>
			<ID>Amperage_Gauge</ID>
			<Title>Amps</Title>
			<Unit></Unit>
			<Minimum>0</Minimum>
			<Maximum>80</Maximum>
			<Style>
					<ForceTextColor>white</ForceTextColor>
					<SizePercent>100</SizePercent>
					<ValuePos>End</ValuePos>
			</Style>
			<Value>
				<If>
					<Condition>
						<Lower>
							<Simvar name="PROP RPM:1" unit="rpm"/>
							<Constant>550</Constant>
						</Lower>
					</Condition>
					<Then>
						<Constant>0</Constant>
					</Then>
					<Else>
						<Simvar name="ELECTRICAL GENALT BUS AMPS:1" unit="amperes"/>
					</Else>
				</If>
			</Value>
			<ColorZone>
				<Color>green</Color>
				<Begin>0</Begin>
				<End>60</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>60</Begin>
				<End>70</End>
			</ColorZone>
			<ColorZone>
				<Color>red</Color>
				<Begin>70</Begin>
				<End>80</End>
			</ColorZone>
			<BeginText></BeginText>
			<EndText></EndText>
			<YellowBlink>
				<And>
					<Greater>
						<Simvar name="PROP RPM:1" unit="rpm"/>
						<Constant>460</Constant>
					</Greater>
					<Greater>
						<Simvar name="ELECTRICAL GENALT BUS AMPS:1" unit="amperes"/>
						<Constant>60</Constant>
					</Greater>
				</And>
			</YellowBlink>
			<RedBlink>
				<And>
					<Greater>
						<Simvar name="PROP RPM:1" unit="rpm"/>
						<Constant>460</Constant>
					</Greater>
					<Greater>
						<Simvar name="ELECTRICAL GENALT BUS AMPS:1" unit="amperes"/>
						<Constant>70</Constant>
					</Greater>
				</And>
			</RedBlink>
		</Gauge>
		
