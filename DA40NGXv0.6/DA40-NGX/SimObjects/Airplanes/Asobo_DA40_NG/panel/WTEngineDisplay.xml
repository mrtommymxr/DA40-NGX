			<RedBlink>
				<Or>
						<Greater>
							<Multiply>
							<Add>
								<Simvar name="RECIP ENG RADIATOR TEMPERATURE:1" unit="celsius"/>
								<Simvar name="GENERAL ENG OIL TEMPERATURE:1" unit="celsius"/>
							</Add>
							<Constant>0.5</Constant>
							</Multiply>
							<Constant>120</Constant>
						</Greater>
				<And>		
						<Lower>
							<Multiply>
							<Add>
								<Simvar name="RECIP ENG RADIATOR TEMPERATURE:1" unit="celsius"/>
								<Simvar name="GENERAL ENG OIL TEMPERATURE:1" unit="celsius"/>
							</Add>
							<Constant>0.5</Constant>
							</Multiply>
							<Constant>-30</Constant>
						</Lower>
						<Greater>
							<Multiply>
							<Add>
								<Simvar name="RECIP ENG RADIATOR TEMPERATURE:1" unit="celsius"/>
