		<Gauge><!--Gearbox_Master-->
			<Type>Horizontal</Type>
			<ID>Piston_CoolantTempGauge</ID>
			<Title>Gearbox</Title>
			<Unit>Â°C</Unit>
			<Minimum>-35</Minimum>
