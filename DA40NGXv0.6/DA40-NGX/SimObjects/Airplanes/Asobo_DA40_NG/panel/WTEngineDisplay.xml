		</Header>

		<Text><!--FF_Master-->
			<Left>FFlow GPH</Left>
			<Right id="Piston_FFlowGauge">
			<If>
				<Condition>
					<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				</Condition>
			<Then>
				<ToFixed precision="1">
					<Simvar name="ENG FUEL FLOW GPH:1" unit="gallons per hour"/>
				</ToFixed>
			</Then>
			<Else>
				<Constant>X</Constant>
			</Else>
			</If>	
			</Right>
		</Text>

		<Text><!--FuelQuantity-->
			<Left>
				<ToFixed precision="0">
					<Min>
					<Simvar name="FUEL LEFT QUANTITY" unit="gallons"/>
					<Constant>14</Constant>
					</Min>
				</ToFixed>
			</Left>
			<Center>Gal</Center>
			<Right>
				<ToFixed precision="0">
					<Min>
					<Simvar name="FUEL RIGHT QUANTITY" unit="gallons"/>
					<Constant>14</Constant>
					</Min>
				</ToFixed>
			</Right>
		</Text>
		
		<Text><!--FuelTemp-->
			<Left>
				<ToFixed precision="0">
				<Substract>
					<Multiply>
						<Add>
							<Simvar name="TOTAL AIR TEMPERATURE:2" unit="celsius"/>
							<Constant>30</Constant>
						</Add>
						<Constant>1.05</Constant>
					</Multiply>
					<Constant>30</Constant>
				</Substract>
				</ToFixed>
			</Left>
			<Center>Â°C</Center>
			<Right>
				<ToFixed precision="0">
				<Simvar name="TOTAL AIR TEMPERATURE:2" unit="celsius"/>
				</ToFixed>
			</Right>
		</Text>
		
